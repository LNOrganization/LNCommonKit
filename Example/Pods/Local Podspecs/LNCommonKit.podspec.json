              "~>1.8.0"
            ]
          }
        },
        {
          "name": "Provider",
          "source_files": "LNCommonKit/Classes/Base/Provider/*",
          "public_header_files": "LNCommonKit/Classes/Base/Provider/*.h",
          "dependencies": {
            "LNCommonKit/Base/Core": [

            ],
            "LNCommonKit/Base/DataModel": [

            ],
            "LNCommonKit/Network": [

            ]
          }
        }
      ]
    },
    {
      "name": "Log",
      "source_files": "LNCommonKit/Classes/Log/**/*",
      "public_header_files": "LNCommonKit/Classes/Log/**/*.h"
    },
    {
      "name": "Consttant",
      "source_files": "LNCommonKit/Classes/Consttant/**/*",
      "public_header_files": "LNCommonKit/Classes/Consttant/**/*.h"
    }
  ]
}
