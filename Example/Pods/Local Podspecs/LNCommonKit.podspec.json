{
  "name": "LNCommonKit",
  "version": "1.0.1",
  "summary": "基础库组件，用于基础服务层代码",
  "description": "基础库组件，用于基础服务层代码，包括网络、日志、工具类等。还包括app所依赖的第三方库，放在这里方便统一管理",
  "homepage": "https://github.com/LNOrganization/LNCommonKit",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "dongjianxiong": "jianxiong20090919@126.com"
  },
  "source": {
    "git": "https://github.com/LNOrganization/LNCommonKit.git",
    "tag": "1.0.1"
  },
  "requires_arc": true,
  "public_header_files": "LNCommonKit/Classes/*.h",
  "source_files": "LNCommonKit/Classes/*",
  "platforms": {
    "ios": "9.0"
  },
  "frameworks": [
    "UIKit",
    "Foundation"
  ],
  "libraries": [
    "z",
    "c++"
  ],
  "xcconfig": {
    "OTHER_LDFLAGS": "-ObjC",
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES"
  },
  "subspecs": [
    {
      "name": "Foundation",
      "source_files": "LNCommonKit/Classes/Foundation/**/*",
      "public_header_files": "LNCommonKit/Classes/Foundation/*.h"
    },
    {
      "name": "CustomUIKit",
      "source_files": "LNCommonKit/Classes/CustomUIKit/**/*",
      "public_header_files": "LNCommonKit/Classes/CustomUIKit/*.h",
      "subspecs": [
        {
          "name": "ImgLoopScroll",
          "source_files": "LNCommonKit/Classes/CustomUIKit/ImgLoopScroll/**/*",
          "public_header_files": "LNCommonKit/Classes/CustomUIKit/ImgLoopScroll/*.h"
        }
      ]
    },
    {
      "name": "Router",
      "source_files": "LNCommonKit/Classes/Router/**/*",
      "public_header_files": "LNCommonKit/Classes/Router/*.h"
    },
    {
      "name": "Network",
      "source_files": "LNCommonKit/Classes/Network/**/*",
      "public_header_files": "LNCommonKit/Classes/Network/**/*.h",
      "subspecs": [
        {
          "name": "Core",
          "source_files": "LNCommonKit/Classes/Network/Core/*",
          "public_header_files": "LNCommonKit/Classes/Network/Core/*.h"
        },
        {
          "name": "LNHTTPClient",
          "source_files": "LNCommonKit/Classes/Network/LNHTTPClient/*",
          "public_header_files": "LNCommonKit/Classes/Network/LNHTTPClient/*.h",
          "dependencies": {
            "LNCommonKit/Network/Core": [

            ]
          }
        },
        {
          "name": "AFHTTPClient",
          "source_files": "LNCommonKit/Classes/Network/AFHTTPClient/*",
          "public_header_files": "LNCommonKit/Classes/Network/AFHTTPClient/*.h",
          "dependencies": {
            "LNCommonKit/Network/Core": [

            ],
            "AFNetworking": [
              "~>4.0.1"
            ]
          }
        }
      ]
    },
    {
      "name": "Base",
      "source_files": "LNCommonKit/Classes/Base/**/*",
      "public_header_files": "LNCommonKit/Classes/Base/**/*.h",
      "subspecs": [
        {
          "name": "BaseVC",
          "source_files": "LNCommonKit/Classes/Base/BaseVC/*",
          "public_header_files": "LNCommonKit/Classes/Base/BaseVC/*.h",
          "dependencies": {
            "LNCommonKit/CustomUIKit": [

            ]
          }
        },
        {
          "name": "Core",
          "source_files": "LNCommonKit/Classes/Base/Core/*",
          "public_header_files": "LNCommonKit/Classes/Base/Core/*.h"
        },
        {
          "name": "TableVC",
          "source_files": "LNCommonKit/Classes/Base/TableVC/*",
          "public_header_files": "LNCommonKit/Classes/Base/TableVC/*.h",
          "dependencies": {
            "LNCommonKit/Base/BaseVC": [

            ],
            "LNCommonKit/Base/Core": [

            ],
            "MJRefresh": [
              "~>3.7.2"
            ]
          }
        },
        {
          "name": "CollectionVC",
          "source_files": "LNCommonKit/Classes/Base/TableVC/*",
          "public_header_files": "LNCommonKit/Classes/Base/TableVC/*.h",
          "dependencies": {
            "LNCommonKit/Base/BaseVC": [

            ],
            "LNCommonKit/Base/Core": [

            ],
            "MJRefresh": [
              "~>3.7.2"
            ]
          }
        },
        {
          "name": "DataModel",
